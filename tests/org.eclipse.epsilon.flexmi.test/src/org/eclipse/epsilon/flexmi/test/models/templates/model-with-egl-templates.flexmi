<?nsuri http://www.eclipse.org/emf/2002/Ecore?>
<_>
	<package name="p1">
		<t1/>
	</package>
	
	<package name="p2">
		<t2/>
	</package>
	
	<package name="p3">
		<t3 n="5" abstract="true"/>
	</package>

	<package name="p4">
		<_t3 _n="5" abstract="true"/>
	</package>
	
	<_template name="t1">
		<content language="EGL">
		<![CDATA[		
			[%for (i in 1.to(10)){%]
			<class name="C[%=i%]"/>
			[%}%]
		]]>
		</content>
	</_template>
	
	<_template name="t2">
		<content language="EGL">
		<![CDATA[		
			<class name="C[%=11%]"/>
		]]>
		</content>
	</_template>
	
	<_template name="t3">
		<parameter name="n"/>
		<content language="EGL">
		<![CDATA[
			[%for (i in 1.to(n.asInteger())){%]
			<class name="C[%=i%]"/>
			[%}%]
		]]>
		</content>
	</_template>
	
</_>
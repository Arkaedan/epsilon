@greedy
rule Component2Dot 
	transform r : Component {
	
	template : "comp2dot.egl"

	target : "graphviz/" + r.name + ".dot"
}

rule Model2HTML {
	
	template : "html.egl"
	
	target : "index.html"
	
}

rule Model2Rundot {
	
	template : "rundot.egl"
	
	target : "graphviz/rundot.sh"
	
}
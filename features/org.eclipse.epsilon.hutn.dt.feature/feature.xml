        This program and the accompanying materials are made
        available under the terms of the Eclipse Public License 2.0
        which is available at https://www.eclipse.org/legal/epl-2.0
   </copyright>

   <includes
         id="org.eclipse.epsilon.dependencies.feature"
         version="0.0.0"/>

   <requires>
      <import plugin="org.eclipse.epsilon.hutn.engine"/>
      <import plugin="org.eclipse.epsilon.common.dt"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.epsilon.emc.emf"/>
      <import plugin="org.eclipse.emf.ecore.editor"/>
      <import plugin="org.eclipse.epsilon.emc.composite"/>
      <import plugin="org.eclipse.epsilon.hutn.xmi"/>
      <import plugin="org.eclipse.epsilon.emc.hutn"/>
      <import plugin="org.eclipse.epsilon.emf.dt"/>
   </requires>

   <plugin
         id="org.eclipse.epsilon.hutn.dt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.epsilon.hutn.xmi.dt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.epsilon.emc.hutn.dt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>

<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.epsilon.hutn.dt.feature"
      label="Human Usable Textual Notation Development Tools"
      version="1.6.0.qualifier"
      provider-name="Eclipse Modeling Project"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <description url="http://www.eclipse.org/epsilon">
      This feature provides an editor and supporting tools for models specified in the OMG HUTN textual notation. These models can be accessed by Epsilon programs through the a HUTN EMC driver.
   </description>


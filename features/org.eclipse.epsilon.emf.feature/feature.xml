        This program and the accompanying materials are made
        available under the terms of the Eclipse Public License 2.0
        which is available at https://www.eclipse.org/legal/epl-2.0
   </copyright>

   <includes
         id="org.eclipse.epsilon.dependencies.feature"
         version="0.0.0"/>

   <requires>
      <import plugin="org.eclipse.epsilon.eol.engine"/>
      <import plugin="org.eclipse.emf"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.emf.ecore.change"/>
   </requires>

   <plugin
         id="org.eclipse.epsilon.emc.emf"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.epsilon.emc.emf.virtual"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.epsilon.emc.emf.decorator"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>

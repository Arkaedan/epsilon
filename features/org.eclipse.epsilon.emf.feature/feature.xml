<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.epsilon.emf.feature"
      label="Epsilon EMF Integration"
      version="1.6.0.qualifier"
      provider-name="Eclipse Modeling Project"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <description url="http://www.eclipse.org/epsilon">
      This feature provides the Epsilon Model Connectivity driver for EMF models. This driver is required in order to work with EMF models in Epsilon.
   </description>

